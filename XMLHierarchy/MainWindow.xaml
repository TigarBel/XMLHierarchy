<Window x:Class="XMLHierarchy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XMLHierarchy"
        mc:Ignorable="d"
        Title="XMLH" Height="450" Width="800" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="169*"/>
            <ColumnDefinition Width="623*"/>
        </Grid.ColumnDefinitions>
        <TreeView x:Name="treeViewXMLH" HorizontalAlignment="Left" Height="369" Margin="10,40,0,0" VerticalAlignment="Top" Width="772" Grid.ColumnSpan="2"/>
        <TextBox x:Name="textBoxURI" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="692" Grid.ColumnSpan="2"/>
        <Button x:Name="buttonStart" Content="Start" Grid.Column="1" HorizontalAlignment="Left" Margin="538,10,0,0" VerticalAlignment="Top" Width="75" Click="ButtonStart_Click"/>

    </Grid>
</Window>
